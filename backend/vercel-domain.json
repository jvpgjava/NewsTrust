{
  "version": 2,
  "name": "newstrust-api",
  "builds": [
    {
      "src": "server-vercel.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server-vercel.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "server-vercel.js": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "domains": ["api.newstrust.me"]
}
