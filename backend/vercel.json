{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "installCommand": "npm install --production=false",
  "buildCommand": "echo 'No build step required'",
  "outputDirectory": ".",
  "functions": {
    "server.js": {
      "maxDuration": 30
    }
  },
  "includeFiles": [
    "test/**/*"
  ]
}